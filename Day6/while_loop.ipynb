      "Starting hurdles!\n",
      "6 - Jump!\n",
      "5 - Jump!\n",
      "4 - Jump!\n",
      "3 - Jump!\n",
      "2 - Jump!\n",
      "1 - Jump!\n",
      "Hurdles finished!\n"
     ]
    }
   ],
   "source": [
    "number_of_hurdles = 6\n",
    "\n",
    "print(\"Starting hurdles!\")\n",
    "while number_of_hurdles > 0:\n",
    "    print(str(number_of_hurdles) + \" - Jump!\")\n",
    "    number_of_hurdles -= 1\n",
    "\n",
    "print(\"Hurdles finished!\")"
   ]
  }
